#!/usr/bin/python3
from math import *
from sys import *

def find_factor(n):
        if (n % 2 == 0):
                return (2)
        for i in range(3, isqrt(n) + 1, 2):
                if (n % i == 0):
                        return (i)

if __name__=='__main__':
        f = open(argv[1],"r")
        for line in f:
                n = int(line)
                q = find_factor(n)
                p = n // q
                print("{:d}={:d}*{:d}".format(n, p, q))